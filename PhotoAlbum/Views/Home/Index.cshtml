@model List<PhotoAlbum.Models.Photo>

@{
    ViewData["Title"] = "Home Page";
}
<h1>All Photos</h1>
@foreach(var item in Model)
{
    <div>
        <h2>@item.Title</h2>
        <p>
            @item.PhotoId<br />
            @item.Description<br />
            @item.CreateDate<br />
            @item.Filename
        </p>
    </div>
}
